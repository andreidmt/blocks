{
  "name": "@mutantlove/blocks",
  "version": "0.7.0",
  "description": "request |> think hard |> response",
  "homepage": "https://github.com/mutantlove/blocks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/blocks.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/blocks/issues"
  },
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check -u",
    "----BUILD": "",
    "build": "rm -rf dist && babel src -d dist",
    "----LINT": "",
    "lint:js": "eslint --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "start": "nodemon -r @babel/register examples/simple.js",
    "test": "node -r @babel/register tests/index.js | tap-nirvana",
    "tdd": "nodemon --watch src --watch tests --watch examples --exec \"npm test\"",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "nodejs",
    "api",
    "server",
    "framework"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@mutantlove/m": "^1.8.0",
    "@mutantlove/pluginus": "^1.0.0",
    "@sindresorhus/slugify": "^0.11.0",
    "accepts": "^1.3.7",
    "ajv": "^6.11.0",
    "busboy": "^0.3.1",
    "connect": "^3.7.0",
    "content-type": "^1.0.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "^4.1.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "@mutantlove/eslint-config": "5.8.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-no-inferred-method-name": "2.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "16.1.1",
    "form-data": "3.0.0",
    "markdownlint-cli": "0.22.0",
    "node-fetch": "2.6.0",
    "nodemon": "2.0.2",
    "npm-check": "5.9.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "riteway": "6.1.1",
    "semantic-release": "17.0.4",
    "tap-nirvana": "1.1.0"
  }
}
