{
  "name": "@mutantlove/blocks",
  "version": "0.7.0",
  "description": "request |> think hard |> response",
  "homepage": "https://github.com/mutantlove/blocks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/blocks.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/blocks/issues"
  },
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check -u --save-exact",
    "----BUILD": "",
    "build": "rm -rf dist && babel src -d dist",
    "----LINT": "",
    "lint:js": "eslint --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "start": "nodemon -r @babel/register examples/simple.js",
    "test": "node -r @babel/register tests/index.js | tap-nirvana",
    "tdd": "nodemon --watch src --watch examples --exec \"npm test\"",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "nodejs",
    "api",
    "server",
    "framework"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "7.7.2",
    "@mutantlove/m": "1.4.0",
    "@mutantlove/pluginus": "0.9.0",
    "ajv": "6.10.2",
    "connect": "3.7.0",
    "cors": "2.8.5",
    "cuid": "2.1.6",
    "debug": "4.1.1",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "path-to-regexp": "6.1.0",
    "qs": "6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/register": "7.7.0",
    "@mutantlove/eslint-config": "5.4.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.8",
    "dotenv": "8.2.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-no-inferred-method-name": "2.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "13.0.0",
    "markdownlint-cli": "0.19.0",
    "nodemon": "2.0.0",
    "npm-check": "5.9.0",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "prom-client": "11.5.3",
    "request": "2.88.0",
    "request-promise": "4.2.5",
    "riteway": "6.1.1",
    "semantic-release": "15.13.31",
    "tap-nirvana": "1.1.0"
  }
}
